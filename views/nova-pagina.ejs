<div class="card">
    <h2><%= title %></h2>
    <p><%= description %></p>
</div>

<div class="card">
    <h3>Dados personalizados</h3>
    <p><%= customData %></p>

    <h4>Lista de itens:</h4>
    <ul>
        <% items.forEach(item => { %>
            <li><%= item %></li>
        <% }) %>
    </ul>
</div>

<div class="card">
    <h3>Teste de Funcionalidades</h3>
    <button class="btn" onclick="testarFuncao()">Testar Função</button>
</div>

<script>
    function testarFuncao() {
        alert('Função testada com sucesso!');
    }
</script>